<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="user name" id="input_user_name" type="text" class="validate">
                <label for="first_name">User name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="input_password" type="password" class="validate">
                <label for="password">Password</label>
            </div>
        </div>
        <div class="row">
            <div class="col s12">
                <button id=login_submit type="submit" class="btn btn-default">Login</button>
            </div>
        </div>
        <div id="link">
            <span>If you do not have an account, please signup </span><a id="signup" href="#">here</a>
        </div>
    </form>
</div>
