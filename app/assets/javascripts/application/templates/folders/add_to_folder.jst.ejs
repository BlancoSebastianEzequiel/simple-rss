<div id="add_to_folder_form_modal" class="modal">
    <div id="modal_body">
        <div class="modal-content">
            <div id="user_folders_list"> </div>
            <span id="name_error" class="helper-text-custom" data-error="wrong" data-success="right"><%= nameError %></span>
        </div>
        <div class="modal-footer">
            <div id="go_to_button"></div>
            <button class="right waves-effect waves-light btn" id="add_to_folder_submit"> Confirm</button>
            <button id="cancel" class="right modal-close waves-effect waves-green btn-flat">Cancel</button>
        </div>
    </div>
</div>
