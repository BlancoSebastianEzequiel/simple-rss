<h4 class="modal-title">New folder</h4>
<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input id="input_name" type="text" class="validate">
                <label for="name">name</label>
                <span id="name_error" class="helper-text-custom" data-error="wrong" data-success="right"><%= nameError %></span>
            </div>
        </div>
        <div class="row">
            <div class="col s12">
                <button id="new_folder_name_submit" type="submit" class="btn btn-default">Ok</button>
            </div>
        </div>
    </form>
</div>
